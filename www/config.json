{
    "locals": {
        "url": "http://localhost:8080",
        "name": "The Wintersmith's blog",
        "owner": "Someone",
        "description": "Ramblings of an immor(t)al demigod"
    },
    "plugins": [
        "./plugins/paginator.coffee",
        "./plugins/wintersmith-handlebars.coffee"
    ],
    "require": {
        "moment": "moment",
        "_": "lodash",
        "typogr": "typogr"
    },
    "jade": {
        "pretty": true
    },
    "handlebars": {
        "partialDir": "partials",
        "helperDir": "helpers"

    },
    "markdown": {
        "smartLists": true,
        "smartypants": true
    },
    "paginator": {
        "template": "index.html",
        "first": "index.html",
        "perPage": 3
    }
}