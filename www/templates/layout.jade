doctype html
block vars
  - var bodyclass = null;
html(lang='en')
  head
    block head
      meta(charset='utf-8')
      meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
      meta(name='viewport', content='width=device-width')
      title
        block title
          = locals.name
      link(rel='alternate', href=locals.url+'/feed.xml', type='application/rss+xml', title=locals.description)
      //- link(rel='stylesheet', href='http://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic|Anonymous+Pro:400,700,400italic,700italic|Merriweather:400,700,300')
      link(rel='stylesheet', href='/css/style.css')   

      script(type="text/javascript", src="/js/jquery.min.js")      
      script(type="text/javascript", src="/js/nav-resp.js")

      link(rel="shortcut icon", href="/images/favicon.ico")

  body
    div#wrap.colorskin-0
      header#header
        div.container          
          div.six.columns.logo            
            a(href="index.html")
              h4 User-centered Design and Development (UCCD) II
          nav#nav-wrap.nav-wrap1.eight.columns
            ul#nav.disc
              li.current
                a(href="#") Home            
              li
                a(href="#") Challenges            
              li
                a(href="#") Hackathons
                 
      section#headline
        .container
          h3= page.metadata.name
          button.small.teal(type="button")
            span.icomoon-calendar-4= moment(page.metadata.date).format("MMM Do YY")
            |, 5:15pm - 7:45pm
          button.small.gray(type="button")
            span.icomoon-location-6= page.metadata.location

      div#content        
        section.container.page-content
          hr.vertical-space2
          block content            
      
      footer#footer
        div.content-wrap.container
          block footer          
            div footer
          
